{
    "name": "uniqkey/laravel-scim-server",
    "description": "Laravel Package for creating a SCIM server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arie Timmerman",
            "email": "arietimmerman@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.7.*",
        "tmilos/scim-schema": "^0.1.0",
        "tmilos/scim-filter-parser": "^1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7",
        "bamarni/composer-bin-plugin": "^1.2",
        "localheinz/composer-normalize": "^1.0",
        "nunomaduro/collision": "^2.1"
    },
    "classmap": [
        "database/migrations"
    ],
    "autoload": {
        "psr-4": {
            "ArieTimmerman\\Laravel\\SCIMServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArieTimmerman\\Laravel\\SCIMServer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArieTimmerman\\Laravel\\SCIMServer\\ServiceProvider"
            ],
            "aliases": {
                "SCIMServerHelper": "ArieTimmerman\\Laravel\\SCIMServer\\Helper"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 1 ] && ls -la vendor/bin && composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 1 ] && composer bin all update --ansi"
        ]
    }
}
